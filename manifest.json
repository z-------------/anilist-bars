{
  "manifest_version": 2,
  "name": "Recent Progress Overview for AniList",
  "version": "0.1.0",
  "permissions": [
    "storage"
  ],
  "content_scripts": [{
    "matches": [ "*://anilist.co/*" ],
    "css": [ "style.css" ],
    "js": [ "getSettings.js", "scripts/common.js", "scripts/bars.js", "scripts/hovercards.js" ]
  }],
  "web_accessible_resources": [
    "defaults.json"
  ],
  "options_ui": {
    "page": "options/index.html",
    "open_in_tab": false
  }
}
